{
  "cli": {
    "version": ">= 1.0.0"
  },
  "build": {
    "dev": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_VARIANT": "development"
      }
    },
    "beta": {
      "releaseChannel": "beta",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_VARIANT": "beta"
      }
    },
    "prod": {
      "releaseChannel": "default",
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
